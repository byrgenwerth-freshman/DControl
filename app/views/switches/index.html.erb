<h1>Listing switches</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Model</th>
    <th>Cisco pro name</th>
    <th>Cisco pro com</th>
    <th>Sys desc</th>
    <th>Sys name</th>
    <th>Sys uptime</th>
    <th>Gathered</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @switches.each do |switch| %>
  <tr>
    <td><%= switch.id %></td>
    <td><%= switch.model %></td>
    <td><%= switch.cisco_pro_name %></td>
    <td><%= switch.cisco_pro_com %></td>
    <td><%= switch.sys_desc %></td>
    <td><%= switch.sys_name %></td>
    <td><%= switch.sys_uptime %></td>
    <td><%= switch.gathered %></td>
    <td><%= switch.location %></td>
    <td><%= link_to 'Show', switch %></td>
    <td><%= link_to 'Edit', edit_switch_path(switch) %></td>
    <td><%= link_to 'Destroy', switch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Switch', new_switch_path %>
